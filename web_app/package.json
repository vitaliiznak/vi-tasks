{
  "name": "web_app_company",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "BROWSER=none vite",
    "build": "tsc && vite build",
    "start:with:error-boundary": "BROWSER=none NODE_ENV=production VITE_APP_IS_ERROR_BOUNDARY=1 vite",
    "build:dev": "dotenv -e .development.env tsc && vite build",
    "build:prod": "dotenv -e .prod.env tsc && vite build",
    "dotenv": "dotenv",
    "lint:fix": "eslint --fix --ext .tsx --ext .ts  .",
    "serve:build": "serve ./build",
    "eslint": "eslint",
    "apollo": "apollo",
    "apollo:schema-download": "npx apollo service:download --endpoint=http://localhost:4000/graphql",
    "apollo:generate-types": "npx graphql-codegen --config ./codegen.yml",
    "populate-api:dev": "dotenv -e .development.env ts-node src/scripts/api_populate"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "4.7.0",
    "@apollo/client": "~3.6.7",
    "@emotion/css": "^11.9.0",
    "@ngneat/falso": "^5.7.0",
    "antd": "^4.21.0",
    "antd-img-crop": "~4.2.3",
    "apollo-upload-client": "^17.0.0",
    "moment": "^2.29.3",
    "npm": "^8.12.1",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.1.0",
    "react-router-dom": "^6.3.0",
    "react-router-hash-link": "^2.4.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/typescript-operations": "^2.4.2",
    "@rollup/plugin-alias": "^3.1.9",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "~13.3.0",
    "@types/apollo-upload-client": "~17.0.0",
    "@types/jest": "^28.1.1",
    "@types/node": "^17.0.2",
    "@types/node-fetch": "^2.6.1",
    "@types/node-localstorage": "^1.3.0",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "@types/react-router-hash-link": "^2.4.5",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "~5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "@vitejs/plugin-react": "^1.3.2",
    "babel-plugin-import": "^1.13.5",
    "customize-cra": "~1.0.0",
    "dotenv": "^16.0.1",
    "dotenv-cli": "~5.1.0",
    "eslint": "^8.17.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-sonarjs": "^0.13.0",
    "less": "^4.1.3",
    "node-localstorage": "^2.2.1",
    "react": "^18.1.0",
    "react-app-rewired": "^2.2.1",
    "serve": "^13.0.2",
    "ts-node": "^10.8.1",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.3",
    "vite": "^2.9.12",
    "vite-plugin-imp": "^2.2.0",
    "vite-plugin-svgr": "^2.1.0"
  },
  "resolutions": {
    "react": "18.1.0"
  }
}