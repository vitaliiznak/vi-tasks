{
  "name": "web_app_company",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "BROWSER=none react-scripts start",
    "start:with:error-boundary": "BROWSER=none NODE_ENV=production REACT_APP_IS_ERROR_BOUNDARY=1 react-scripts start",
    "build": "react-scripts build",
    "build:dev": "dotenv -e .development.env react-scripts build",
    "build:prod": "dotenv -e .prod.env react-scripts build",
    "dotenv": "dotenv",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint:fix": "eslint --fix --ext .tsx --ext .ts  .",
    "serve": "serve",
    "serve:build": "serve ./build",
    "eslint": "eslint",
    "apollo": "apollo",
    "apollo:schema-download": "apollo service:download --endpoint=http://localhost:4000/graphql",
    "apollo:generate-types": "npx graphql-codegen --config ./codegen.yml",
    "populate-api:dev": "dotenv -e .development.env ts-node src/scripts/api_populate"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@apollo/client": "~3.5.10",
    "@emotion/css": "^11.9.0",
    "@ngneat/falso": "^5.0.0",
    "antd": "^4.20.0",
    "antd-img-crop": "~4.2.0",
    "apollo-upload-client": "^17.0.0",
    "moment": "^2.29.3",
    "react-beautiful-dnd": "^13.1.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.3.0",
    "react-router-hash-link": "^2.4.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/typescript-operations": "^2.3.5",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "~13.1.1",
    "@types/apollo-upload-client": "~17.0.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.25",
    "@types/node-fetch": "^2.6.1",
    "@types/node-localstorage": "^1.3.0",
    "@types/react": "^18.0.5",
    "@types/react-beautiful-dnd": "^13.1.2",
    "@types/react-dom": "^18.0.2",
    "@types/react-router-hash-link": "^2.4.5",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "~5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "babel-plugin-import": "^1.13.5",
    "customize-cra": "~1.0.0",
    "dotenv": "^16.0.0",
    "dotenv-cli": "~5.1.0",
    "eslint": "^8.14.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "eslint-plugin-sonarjs": "^0.13.0",
    "less": "^4.1.2",
    "node-localstorage": "^2.2.1",
    "react": "^18.0.0",
    "react-app-rewired": "^2.2.1",
    "react-scripts": "^5.0.1",
    "serve": "^13.0.2",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.6.3"
  },
  "resolutions": {
    "react": "18.0.0"
  }
